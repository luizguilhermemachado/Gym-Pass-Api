    "@types/node": "24.3.0",
    "@types/supertest": "6.0.3",
    "@vitest/ui": "3.2.4",
    "eslint": "8.57.1",
    "prisma": "6.15.0",
    "supertest": "7.1.4",
    "tsup": "8.5.0",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/jwt": "10.0.0",
